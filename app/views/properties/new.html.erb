<%= form_for @property do |f| %>
  <% if @property.errors.any? %>
    <div id = "error_explanation">
      <h2><%= @property.errors.count %>件の件エラーがあります</h2>
      <ul>
      <% @property.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>    
  <% end %>
  <h2>物件登録</h2>
  <dl>
    <dt>物件名</dt>
    <dt>賃料</dt>
    <dt>住所</dt>
    <dt>築年数</dt>
    <dt>備考</dt>
  </dl>

  <h3>最寄り駅</h3>
  <dl>
    <dt>路線名</dt>
    <dt>駅名</dt>
    <dt>徒歩分数</dt>
  </dl>
  <% end %>

